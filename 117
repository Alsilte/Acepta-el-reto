import java.util.Scanner;

public tintin{

    public static void main (String[]args){

        int n;
        String nombre;
        String soy;

        Scanner sc = new Scanner (System.in);

        n = sc.nextInt();
        
        for (int i = 0; i<n; i++){
            soy = sc.next();
            nombre= sc.nextln();
            System.out.print("Hola, "+ nombre+".");
            
        }
    }
}
